		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
		<rdfs:domain rdf:resource="http://ontology.un.org/undoc#Body"/>
		<rdfs:label>appoints</rdfs:label>
		<rdfs:range rdf:resource="http://ontology.un.org/undoc#Official"/>
