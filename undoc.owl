<?xml version="1.0"?>
<rdf:RDF xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:foaf="http://ontology.un.org/undoc#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
	<!-- //Ontology declaration// -->
	<rdf:Description rdf:about="http://ontology.un.org/undoc">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
		<rdfs:label>United Nations Documentation Ontology</rdfs:label>
	</rdf:Description>
	<!-- //Classes// -->
	<rdf:Description rdf:about="http://ontology.un.org/undoc#MemberState">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes a member state of the UN</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Organization">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes an organization within the UN, composed of member states</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Body">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes a body within the UN, established by an Organization</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Session">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes a session of the UN held by a body</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Agenda">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes a session agenda</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#AgendaItem">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes an agenda item</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Official">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes the UN official who, at the behest of a body, writes a document</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Speech">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes a speech given by a member state of the UN</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Vote">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes a vote by a member state of the UN</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Resolution">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes resolution/decision or draft resolution/decision sponsored by a member state of the UN</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#Document">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes a document written by a UN official as the result of a decision/resolution</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#AgendaSubject">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
		<rdfs:comment xml:lang="en">Denotes an agenda subject</rdfs:comment>
		<rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://ontology.un.org/undoc</rdfs:isDefinedBy>
	</rdf:Description>
	<!-- //Object Properties// -->
	<rdf:Description rdf:about="http://ontology.un.org/undoc#memberOfOrganization">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
		<rdfs:domain rdf:resource="http://ontology.un.org/undoc#MemberState"/>
		<rdfs:label>is member of organization</rdfs:label>
		<rdfs:range rdf:resource="http://ontology.un.org/undoc#Organization"/>
		<owl:inverseOf rdf:resource="http://ontology.un.org/undoc#hasMemberState"/>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#hasMemberState">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
		<rdfs:domain rdf:resource="http://ontology.un.org/undoc#Organization"/>
		<rdfs:label>has member state</rdfs:label>
		<rdfs:range rdf:resource="http://ontology.un.org/undoc#MemberState"/>
		<owl:inverseOf rdf:resource="http://ontology.un.org/undoc#memberOfOrganization"/>
	</rdf:Description>
	<rdf:Description rdf:about="http://ontology.un.org/undoc#memberOfBody">
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
