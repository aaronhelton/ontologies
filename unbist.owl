<?xml version="1.0"?>
<rdf:RDF xmlns="http://ontology.un.org/unbist#" 
	xml:base="http://ontology.un.org/unbist"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:skosxl="http://www.w3.org/2008/05/skos-xl#"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
	<owl:Ontology rdf:about="">
		<rdfs:comment xml:lang="en">EuroVoc imports to UNBIS Thesaurus based on SKOS and SKOS-XL. 
Adds the following Classes from EuroVoc: Domain, MicroThesaurus
Adds the following Object Properties from EuroVoc: domain, microThesaurus
Adds the following Datatype Properties from EuroVoc: hasPolyHierarchy
		</rdfs:comment>
	</owl:Ontology>
	
	<!-- //Classes not included in SKOS or SKOS-XL// -->
	
    <!-- http://eurovoc.europa.eu/schema#Domain -->
    <owl:Class rdf:about="http://eurovoc.europa.eu/schema#Domain">
        <rdfs:label xml:lang="en">domain</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/identifier"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <owl:disjointWith rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <owl:disjointWith rdf:resource="http://www.w3.org/2008/05/skos-xl#Label"/>
    </owl:Class>

    <!-- http://eurovoc.europa.eu/schema#MicroThesaurus -->
    <owl:Class rdf:about="http://eurovoc.europa.eu/schema#MicroThesaurus">
        <rdfs:label xml:lang="en">microthesaurus</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/elements/1.1/identifier"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>	

	<!-- //Object Properties not included in SKOS or SKOS-XL// -->
	
	<!-- http://eurovoc.europa.eu/schema#domain -->
    <owl:ObjectProperty rdf:about="http://eurovoc.europa.eu/schema#domain">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">domain</rdfs:label>
        <rdfs:range rdf:resource="http://eurovoc.europa.eu/schema#Domain"/>
        <rdfs:domain rdf:resource="http://eurovoc.europa.eu/schema#MicroThesaurus"/>
    </owl:ObjectProperty>
	
	<!-- http://eurovoc.europa.eu/schema#microThesaurus -->
    <owl:ObjectProperty rdf:about="http://eurovoc.europa.eu/schema#microThesaurus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:label xml:lang="en">micro thesaurus</rdfs:label>
        <rdfs:domain rdf:resource="http://eurovoc.europa.eu/schema#Domain"/>
        <rdfs:range rdf:resource="http://eurovoc.europa.eu/schema#MicroThesaurus"/>
        <owl:inverseOf rdf:resource="http://eurovoc.europa.eu/schema#domain"/>
    </owl:ObjectProperty>
	
	<!-- //Datatype Properties not included in SKOS or SKOS-XL// -->
	
	<!-- http://eurovoc.europa.eu/schema#hasPolyHierarchy -->
    <owl:DatatypeProperty rdf:about="http://eurovoc.europa.eu/schema#hasPolyHierarchy">
        <rdfs:label xml:lang="en"
            >has poly hierarchy</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://eurovoc.europa.eu/schema#Domain"/>
                    <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
</rdf:RDF>
